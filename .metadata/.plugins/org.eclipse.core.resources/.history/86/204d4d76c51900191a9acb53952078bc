package abstractfun;

public class Jungle {

	public static void main(String[] args) {
		
		//Animal a = new Animal(); //Can't make *just* an animal
		Lion l = new Lion();
		
		//Can still label a lion as an animal, tho
		Animal leo = new Lion();
		
		//a.makeSound();
		l.makeSound();
		leo.makeSound();

	}

}
